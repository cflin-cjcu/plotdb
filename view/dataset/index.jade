extends ../module/base.jade
block vars
  - var rootid = "dataview"
  - var title = 'plotdb'
  - var description = "plotdb"
  - var thumbnail = "http://plotdb.io/assets/img/thumbnail.jpg"
  - var thumbtype = "image/jpg"
  - var url = "http://plotdb.com/"
block head
  title #{dataset.name} by #{dataset.ownerName}
  script(type="text/javascript").
    window.dataset = !{JSON.stringify(dataset)};
block body
  span &nbsp;
  #root.narrow
    include ../module/dataset/create.jade
  include ../module/footer.jade
