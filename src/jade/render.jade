doctype html
//- basic libraries: jquery(1.10.2), d3js(3.3.11), angularjs(1.2.3), bootstrap(3.0)/semantic-ui(0.9.3/0.9.5)
- var version = "26"
- var assets = "/assets"
- var thumbnail = "http://plotdb.com/assets/img/thumbnail.png"
- var thumbtype = "image/png"
- var thumb_width = "1200"
- var thumb_height = "630"
- var favicon = assets + "/img/favicon-16.png"
- var rootid = "renderview"
html(lang="en")
  block vars
  head(prefix="og: http://ogp.me/ns#")
    //-include library.jade
    block head
    meta(charset="utf-8")
    meta(name="script",content="/js/pack/render.js")
    style(type="text/css").
      html,body,#wrapper,#container{width:100%;height:100%;margin:0}body{background:#fff;font-family:'Helvetica Neue',Arial,sans}.hover-box,.hover-margin{position:absolute;z-index:99999;pointer-events:none}.hover-box{border:1px dashed red}.hover-margin{border:1px dashed rgba(255,192,0,0.5)}
    include colorblind.jade
  body
    block body
