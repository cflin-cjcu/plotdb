@import 'vars'
@import 'basic'

#chart-explore-bar
  background: #fff
  box-shadow: 0 1px 2px rgba(0,0,0,0.1)
  border: 1px solid #ccc
  font-size: 1.2em
  font-family: 'Lato', 'Helvetica Neue', Helvetica, sans-serif
  .navbar-nav > li > a:hover
    color: #ed1d78
  .navbar-nav > li.open > a
    background: #ed1d78
    color: #fff
  .dropdown-menu > li > a:hover, .dropdown-menu > li > a:focus
    color: #fff
    background: #ed1d78
